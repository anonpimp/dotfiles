{ "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Visual Studio Dark",
  "window.restoreWindows": "all",
  "files.hotExit": "onExit",
  "workbench.startupEditor": "none",
  "extensions.ignoreRecommendations": true,
  "editor.suggestSelection": "first",
  "editor.fontSize": 18,
  "editor.fontLigatures": true,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "editor.fontFamily": "'Juminai', 'Font Awesome 6 Pro', 'Font Awesome 6 Brands', 'Symbols Nerd Font'",
  "editor.minimap.enabled": false,
  "colorize.include": [
    "**/*.*"
  ],
  "colorize.languages": [
    "javascript"
  ],
  "editor.tokenColorCustomizations": {
    "comments": "#4c4c4c",
    "strings": "#9ec49f",
    "functions": "#a39ec4",
    "variables": "#cacaca",
    "keywords": "#ceb188",
    "types": "#c49ea0",
    "numbers": "#ceb188",
    "textMateRules": [
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#ceb188"
        }
      },
      {
        "scope": "support.function",
        "settings": {
          "foreground": "#c49ec4"
        }
      },
      {
        "scope": "constant",
        "settings": {
          "foreground": "#9ec49f"
        }
      }
    ]
  },
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#9ec3c4",
    "editorBracketHighlight.foreground2": "#ceb188",
    "editorBracketHighlight.foreground3": "#a39ec4",
    "editorBracketHighlight.foreground4": "#e06c75",
    "editorBracketHighlight.foreground5": "#c49ec4",
    "editorBracketHighlight.foreground6": "#9ec49f",
    "list.activeSelectionBackground": "#262626",
    "list.hoverBackground": "#262626",
    "editorWidget.background": "#262626",
    "panel.background": "#262626",
    "titleBar.activeBackground": "#262626",
    "breadcrumbPicker.background": "#262626",
    "settings.dropdownBackground": "#262626",
    "settings.numberInputBackground": "#262626",
    "settings.textInputBackground": "#262626",
    "settings.checkboxBackground": "#262626",
    "editor.background": "#191919",
    "tab.activeBackground": "#191919",
    "tab.inactiveBackground": "#0f0f0f",
    "sideBar.background": "#0f0f0f",
    "sideBarSectionHeader.background": "#0f0f0f",
    "activityBar.background": "#0f0f0f",
    "statusBar.background": "#0f0f0f",
    "scrollbarSlider.activeBackground": "#0f0f0f",
    "tab.activeBorder": "#191919",
    "editorGroupHeader.tabsBackground": "#0f0f0f",
    "statusBar.noFolderBackground": "#191919",
    "tab.border": "#0f0f0f",
    "editorHoverWidget.background": "#191919",
    "menu.background": "#191919",
    "debugToolBar.background": "#191919",
    "editorBracketMatch.background": "#191919",
    "quickInput.background": "#191919",
    "editorOverviewRuler.border": "#191919",
    "notifications.background": "#191919",
    "breadcrumb.background": "#191919"
  },
  "files.associations": {
    "*.yuck": "yuck"
  },
  "redhat.telemetry.enabled": true,
}