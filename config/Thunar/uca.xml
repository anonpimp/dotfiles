<action>
	<icon>com.github.gijsgoudzwaard.image-optimizer</icon>
	<name>ImageGoMountain</name>
	<submenu></submenu>
	<unique-id>1653157696807875-1</unique-id>
	<command>python ~/Documents/ImageGoNord/src/cli.py --mountain -i=%n -o=$(ls %f | grep %n | cut -f 1 -d &apos;.&apos;)-mountain</command>
