<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1653152521049373-1</unique-id>
	<command>bspc rule -a \* -o state=floating ; alacritty -e $SHELL</command>
	<description></description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>com.github.maoschanz.DynamicWallpaperEditor</icon>
	<name>Set wallpaper</name>
	<submenu></submenu>
	<unique-id>1653152845925747-1</unique-id>
	<command>feh --bg-fill %n</command>
	<description></description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<image-files/>
</action>
<action>
	<icon>nvim</icon>
	<name>Open with Nvim</name>
	<submenu></submenu>
	<unique-id>1653154187810667-1</unique-id>
	<command>bspc rule -a \* -o state=floating ; alacritty -e nvim %n</command>
	<description></description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>com.github.gijsgoudzwaard.image-optimizer</icon>
	<name>Mountain</name>
	<submenu></submenu>
	<unique-id>1653157696807875-1</unique-id>
	<command>dunstify -i com.github.gijsgoudzwaard.image-optimizer &quot;Wait...&quot; &quot;customizing image&quot; &amp;&amp; python ~/Documents/ImageGoNord/src/cli.py --mountain -i=%n -o=$(ls %f | grep %n | cut -f 1 -d &apos;.&apos;)-mountain</command>
	<description>mountainize a image</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<image-files/>
</action>
</actions>
